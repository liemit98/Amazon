    <div class="content">
      <div id="featured_slide">
        <ul id="featurednews">
          <%for(var i= mess.length-1 ; i>= mess.length-5;i--){%>
          <li><img src="<%= mess[i].image%>" style="width: 100px; height: 100px" alt="" />
            <div class="panel-overlay">
              <h2><%= mess[i].title%></h2>
              <p><%= mess[i].describe%><br />
                <a href="/news/<%= mess[i].idnews%>">Xem chi tiet &raquo;</a></p>
            </div>
          </li>
          <%}%>
        </ul>
      </div>
    </div>
    <div class="column">
      <ul class="latestnews">
        <li><img src="<%= mess[mess.length-3].image%>" style="width: 100px; height: 100px" alt="" />
          <p><strong><a href="/news/<%= mess[mess.length-3].idnews%>"><%= mess[mess.length-3].title%></a></strong><%= mess[mess.length-3].describe%> </p>
        </li>
        <li><img src="<%= mess[mess.length-2].image%>" style="width: 100px; height: 100px" alt="" />
