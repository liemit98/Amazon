    <br class="clear" />
  </div>
  <div id="hpage_cats">
    <%for (var key in mess){%>
    <%if (key % 2 == 0) {%>
    <div class="fl_left">
      <img src="<%= mess[key].image%>" style="width: 100px; height: 100px" alt="" />
